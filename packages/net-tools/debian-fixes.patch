+#if HAVE_AFIPX
+static int ipx_info(void)
+{
+    FILE *f;
+    char buf[256];
+    unsigned long txq, rxq;
+    unsigned int state;
+    unsigned int uid;
+    char *st;
+    int nc;
+    struct aftype *ap;
